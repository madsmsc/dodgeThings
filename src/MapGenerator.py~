class MapGenerator:

    boolean[][] cellmap = new boolean[width][height];

    float chanceToStartAlive = 0.45f;
 
    initialiseMap(map: boolean[][]) -> boolean[][]:
        for x in range(0, width):
            for y in range(0, height):
                if random() < chanceToStartAlive:
                    map[x][y] = True
        return map;

    
